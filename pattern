     1
    232
   34543
  4567654
 567898765

 #include <iostream>
using namespace std;
int main()
{
    for (int i = 0; i < 5; i++)
    {
        for (int k = 5; k > i; k--)
            cout << " ";
        for (int j = 0; j <= i; j++)
        {
            cout << j + i + 1;
        }
        // cout << endl;
        for (int j = i; j > 0; j--)
        {
            cout << j + i;
        }
        cout << endl;
    }
    return 0;
}
